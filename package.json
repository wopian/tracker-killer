{
  "name": "tracker-killer",
  "version": "0.4.0",
  "description": "Library performance tester for anime & manga trackers Edit",
  "license": "MIT",
  "author": "James Harris <wopian@wopian.me>",
  "scripts": {
    "start": "babel-node --presets env,stage-0 src/main.js",
    "test": " "
  },
  "main": "src/main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/wopian/tracker-killer.git"
  },
  "bugs": {
    "url": "https://github.com/wopian/tracker-killer/issues"
  },
  "homepage": "https://github.com/wopian/tracker-killer#readme",
  "dependencies": {
    "anilist-api-pt": "~1.0.0",
    "annict": "~0.0.8",
    "babel-cli": "~6.26.0",
    "babel-preset-env": "~1.6.0",
    "babel-preset-stage-0": "~6.24.1",
    "inquirer": "~5.0.0",
    "kitsu": "~4.4.0",
    "listr": "~0.13.0",
    "moment": "~2.20.0",
    "pad": "~2.0.1",
    "popura": "~1.2.5",
    "simple-node-logger": "~0.93.20",
    "yargs": "~11.0.0"
  },
  "devDependencies": {
    "babel-eslint": "~8.2.0",
    "eslint": "~4.16.0",
    "eslint-config-standard": "~10.2.0",
    "eslint-plugin-import": "~2.8.0",
    "eslint-plugin-node": "~5.2.0",
    "eslint-plugin-promise": "~3.6.0",
    "eslint-plugin-standard": "~3.0.1",
    "phantomas": "~1.19.0",
    "phantomjs-prebuilt": "~2.1.14"
  },
  "renovate": {
    "extends": [
      ":npm",
      ":preserveSemverRanges",
      ":combinePatchMinorReleases",
      ":renovatePrefix",
      ":semanticCommits",
      ":semanticCommitTypeAll(chore)",
      ":rebaseStalePrs",
      ":unpublishSafe",
      ":prNotPending",
      ":automergeDisabled",
      ":maintainLockFilesMonthly",
      ":ignoreUnstable",
      ":updateNotScheduled",
      ":label(dependencies)",
      ":timezone(Europe/London)",
      ":assignee(wopian)",
      "group:babelMonorepo"
    ],
    "prTitle": "update {{depName}} to {{newVersion}}",
    "commitMessage": "update {{depName}} to {{newVersion}}"
  }
}
